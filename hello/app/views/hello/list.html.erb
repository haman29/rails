<table border="1">
  <tr>
    <th>ID</th>
    <th>ISBNコード</th>
    <th>署名</th>
    <th>価格</th>
    <th>出版社</th>
    <th>刊行日</th>
    <th>CD-ROM</th>
  </tr>
  <% @books.each do |book| %>
    <tr>
      <td><%= book.id %></td>
      <td><%= book.isbn %></td>
      <td><%= book.title %></td>
      <td><%= book.price %>円</td>
      <td><%= book.publish %></td>
      <td><%= book.created_at %></td>
      <td><%= book.cd %></td>
    </tr>
  <% end %>
</table>
